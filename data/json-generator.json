[
  {
    'repeat(50, 100)': {
      customerId: "test",
      accountId(tags){
        return "5365635645#" + tags.moment().subtract(tags.integer(1, 11), 	"months").format() + "#" + tags.objectId();
      },
      transactionId: '{{guid()}}',
      isDetailAvailable: false,
      type: '{{random("FEE", "INTEREST_CHARGED", "TRANSFER_INCOMING")}}',
      status: '{{random("PENDING", "POSTED")}}',
      description: '{{lorem(3, "words")}}',
      postingDateTime(tags) {
          return this.status === "POSTED" ? new Date() : "null";
      },
      valueDateTime(tags) {
        return tags.moment().subtract(tags.integer(1, 11), "months").format();
      },
      executionDateTime(tags) {
        return new Date();
      },
      amount(tags){
        return this.type === "PAYMENT" ? tags.floating(1000, 3000, 2, "0.00"): tags.floating(-510, -20, 2, "0.00");
      },
      currency: 'AUD',
      reference: '{{objectId()}}',
      merchantName: '{{company().toUpperCase()}}',
      merchantCategoryCode(tags){
        const v= tags.random("4111x", "5139x", "5411x", "5462x" , "0732x", "null");
        return v;
      }
    }
  }
]